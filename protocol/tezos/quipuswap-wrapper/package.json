{
  "name": "@blockwatch-cc/quipuswap-wrapper",
  "description": "Tezos Quipuswap Polywrapper",
  "private": true,
  "version": "0.0.1-prealpha.1",
  "license": "MIT",
  "scripts": {
    "reset": "yarn clean && yarn build",
    "clean": "npx rimraf ./node_modules ./build ./coverage ./.wrap ./**/wrap",
    "build": "yarn build:polywrap",
    "build:polywrap": "npx polywrap build",
    "test": "yarn test:e2e",
    "test:e2e": "jest --passWithNoTests --runInBand --verbose",
    "test:env:up": "npx polywrap test-env up",
    "test:env:down": "npx polywrap test-env down",
    "deploy": "yarn deploy:polywrap",
    "deploy:polywrap": "npx polywrap build --ipfs http://localhost:5001 --test-ens quipuswap.polywrap.eth"
  },
  "devDependencies": {
    "@taquito/signer": "12.1.0",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.8",
    "polywrap": "0.1.1",
    "@polywrap/client-js": "0.1.1",
    "@polywrap/test-env-js": "0.1.1",
    "@polywrap/wasm-as": "0.1.1",
    "assemblyscript": "0.19.1",
    "date-fns": "^2.28.0",
    "jest": "27.4.7",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4",
    "dotenv": "16.0.0"
  }
}
